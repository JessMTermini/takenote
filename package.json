{
  "name": "takenote",
  "version": "0.7.2",
  "description": "A web-based notes app for developers.",
  "author": "Tania Rascia",
  "license": "MIT",
  "private": false,
  "main": "src/server/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "server": "nodemon --config config/nodemon.config.json",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "prod": "node -r ts-node/register/transpile-only src/server/index.ts",
    "start": "npm run client",
    "test": "jest --config config/jest.config.js",
    "test:e2e": "cypress run --config-file config/cypress.config.json",
    "test:e2e:open": "cypress open --config-file config/cypress.config.json",
    "test:coverage": "jest --config config/jest.config.js --coverage --watchAll=false",
    "test:coverage:ci": "jest --config config/jest.config.js --ci --coverage --watchAll=false && cat ./coverage/lcov.info | coveralls",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "eslint": "eslint src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taniarascia/takenote"
  },
  "keywords": [
    "notes",
    "notes-app",
    "note-taking",
    "markdown",
    "markdown-editor",
    "redux",
    "react",
    "typescript",
    "react-hooks",
    "react-hooks-redux",
    "github"
  ],
  "bugs": {
    "url": "https://github.com/taniarascia/takenote/issues"
  },
  "homepage": "https://takenote.dev",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.21.4",
    "clipboard-polyfill": "^3.0.3",
    "codemirror": "^5.65.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "express": "^4.18.1",
    "helmet": "^4.6.0",
    "jszip": "^3.10.0",
    "mousetrap": "^1.6.5",
    "mousetrap-global-bind": "^1.1.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-codemirror2": "^7.2.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "^16.14.0",
    "react-feather": "^2.0.10",
    "react-helmet-async": "^1.3.0",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.2.8",
    "react-router-dom": "^5.3.3",
    "react-split-pane": "^0.1.92",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "stream-browserify": "^3.0.0",
    "takenote": "^0.1.0",
    "unist-util-visit": "^2.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@testing-library/cypress": "^7.0.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.7",
    "@types/axios": "^0.14.0",
    "@types/codemirror": "0.0.98",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.24",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.18.21",
    "@types/prettier": "^2.6.3",
    "@types/react": "^16.14.26",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^16.9.16",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "clean-webpack-plugin": "^3.0.0",
    "clipboardy": "^2.3.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "cypress": "^5.6.0",
    "cypress-file-upload": "^4.1.1",
    "dotenv": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.30.0",
    "faker": "^5.5.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-raw-loader": "^1.0.1",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.16",
    "sass-loader": "^10.2.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.8.0"
  }
}
